<% if @company.errors.blank? %>
  $('#history .alerts').html("Successfully saved data for company history.").removeClass("alerts-danger");
  $('#history .error-messages').html("");
<% else %>
  $('#history .alerts').html("Could not save data for company history.").addClass("alerts-danger");
  $('#history .error-messages').html("<%= j render partial: 'shared/error_messages', locals: {obj: @company} %>");
<% end %>