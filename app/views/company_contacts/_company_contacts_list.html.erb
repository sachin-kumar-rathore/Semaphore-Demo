<div class="panel">
  <div class="panel-body">
    <div class="row">
      <div class="col-md-7 col-12">
        <h3> Company Contacts </h3>
      </div>
      <div class="col-md-2 col-6">
        <button class="btn btn-sm green-btn waves-effect waves-classic col-md-12 clickable" data-href="<%= new_company_contact_path(@company) %>" type="button" >ADD CONTACT </button>
      </div>
      <div class="col-md-3 col-6">
        <button data-href ="<%= show_existing_contacts_company_contacts_path(@company)%>" class ="btn btn-sm green-btn col-md-12 clickable" >
      ADD EXISTING CONTACT </button>
      </div>
    </div>
  </div>
  <div class="panel-body tableDesign" id="contactsList">
    <%= render partial: "company_contacts/contacts", locals: {contacts: @company_contacts} %>
  </div>
  <div id="createOrEditContact"></div>
</div>