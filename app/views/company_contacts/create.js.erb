<% if @contact.errors.blank? %>
  $('.flashMessages').html("<%= j render partial: 'shared/flash_messages' %>");
  $('#contacts').html("<%= j render partial: 'company_contacts_list' %>");
  $('.modal-backdrop').remove();
  $('#contactFormCenter').modal('hide');
<% else %>
  $('#contactFormCenter .error-messages').html("<%= j render partial: 'shared/error_messages', locals: {obj: @contact} %>");
<% end %>
