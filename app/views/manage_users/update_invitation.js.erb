<% if @user.errors.blank? %>
  $('.settings-tab-headers').html("<%= j render partial: 'shared/settings_tab_headers' %>")
  reloadUsers();
  $('.flashMessages').html("<%= j render partial: 'shared/flash_messages' %>");
<% else %>
  $('#invitationFormCenter .error-messages').html("<%= j render partial: 'shared/error_messages', locals: {obj: @user} %>");
<% end %>