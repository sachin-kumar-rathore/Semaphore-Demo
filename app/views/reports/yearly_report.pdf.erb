<div align="center" id="competitionCompBar"></div><br>

<%
	start_date_or_year = params[:year_to_compare] ? params[:year_to_compare].to_i : 3
	end_date_or_compare = params[:year] ? params[:year].to_i : Date.today.year
	chart_date_range = "From: #{start_date_or_year.year.ago.strftime("%d-%m-%Y")}, To: #{DateTime.new(end_date_or_compare).end_of_year.strftime("%d-%m-%Y")}"
%>

<%= render :partial => 'pdf_scripts', locals: {date_range: chart_date_range} %>