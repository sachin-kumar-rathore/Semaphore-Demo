<div class="row">
  <div class="col-md-6 col-sm-12 ">
    <p class="font-weight-bold">Prospects by Type Comparison</p>
    <div class="example table-responsive">
      <table class="table table-bordered bg-white">
        <thead>
          <tr>
            <th class="font-weight-bold text-center"><%= params[:action] == 'monthly' ? 'Month' : 'Year' %></th>
            <th class="font-weight-bold text-center">Type</th>
            <th class="font-weight-bold text-center">Amount</th>
          </tr>
        </thead>
        <tbody>
          <% results['project_type_id'].keys.each do |year| %>
            <% results['project_type_id'][year].keys.each do |typeId| %>
              <tr>
                <td class="text-center"><%= year %></td>
                <td class="text-center"><%= ProjectType.find(typeId).name %></td>
                <td class="text-center"><%= results['project_type_id'][year][typeId].count %></td>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-md-6 col-sm-12 ">
    <p class="font-weight-bold">Industry Type Comparison</p>
    <div class="example table-responsive">
      <table class="table table-bordered bg-white">
        <thead>
          <tr>
            <th class="font-weight-bold text-center"><%= params[:action] == 'monthly' ? 'Month' : 'Year' %></th>
            <th class="font-weight-bold text-center">Type</th>
            <th class="font-weight-bold text-center">Amount</th>
          </tr>
        </thead>
        <tbody>
          <% results['industry_type_id'].keys.each do |year| %>
            <% results['industry_type_id'][year].keys.each do |typeId| %>
              <tr>
                <td class="text-center"><%= year %></td>
                <td class="text-center"><%= IndustryType.find(typeId).name %></td>
                <td class="text-center"><%= results['industry_type_id'][year][typeId].count %></td>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <div class="col-md-6 col-sm-12">
    <p class="font-weight-bold">Prospect Lead Sources Comparison</p>
    <div class="example table-responsive">
      <table class="table table-bordered bg-white">
        <thead>
          <tr>
            <th class="font-weight-bold text-center"><%= params[:action] == 'monthly' ? 'Month' : 'Year' %></th>
            <th class="font-weight-bold text-center">Type</th>
            <th class="font-weight-bold text-center">Amount</th>
          </tr>
        </thead>
        <tbody>
          <% results['source_id'].keys.each do |year| %>
            <% results['source_id'][year].keys.each do |typeId| %>
              <tr>
                <td class="text-center"><%= year %></td>
                <td class="text-center"><%= Source.find(typeId).name %></td>
                <td class="text-center"><%= results['source_id'][year][typeId].count %></td>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-md-6 col-sm-12 ">
    <p class="font-weight-bold">Reasons or Project Elimination Comparison</p>
    <div class="example table-responsive">
      <table class="table table-bordered bg-white">
        <thead>
          <tr>
            <th class="font-weight-bold text-center"><%= params[:action] == 'monthly' ? 'Month' : 'Year' %></th>
            <th class="font-weight-bold text-center">Type</th>
            <th class="font-weight-bold text-center">Amount</th>
          </tr>
        </thead>
        <tbody>
          <% results['elimination_reason_id'].keys.each do |year| %>
            <% results['elimination_reason_id'][year].keys.each do |typeId| %>
              <% if typeId %>
                <tr>
                  <td class="text-center"><%= year %></td>
                  <td class="text-center"><%= EliminationReason.find_by_id(typeId).try(:name) %></td>
                  <td class="text-center"><%= results['elimination_reason_id'][year][typeId].count %></td>
                </tr>
              <% end %>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-md-6 col-sm-12 ">
    <p class="font-weight-bold">Prospect Acreage Request</p>
    <div class="example table-responsive">
      <table class="table table-bordered bg-white">
        <thead>
          <tr>
            <th class="font-weight-bold text-center"><%= params[:action] == 'monthly' ? 'Month' : 'Year' %></th>
