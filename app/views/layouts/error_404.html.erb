<!DOCTYPE html>
<html>
<head>
  <title>Tracker</title>
  <%= csrf_meta_tags %>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="icon" type="image/x-icon" href="/assets/favicon.ico" />
  <%= stylesheet_link_tag 'application_new', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application_new', 'data-turbolinks-track': 'reload' %>
</head>
<body>
  <header>
    <div class="headerSection">
      <div class="container">
        <div class="logo">
          <a href="<%= logo_redirect_path %>"><%= render '/layouts/logo_svg' %></a>
        </div>
      </div>
    </div>
  </header>
  <section>
    <div class="rightImgBg loginPage">
      <div class="container">
        <div class="formSection">
          <div class="formTitle">
            <h3>404 - Page Not Found</h3>
          </div>
          <div class="formStyle">
            <%= link_to 'Home', root_path, class: 'btn primaryBtn', data: { turbolinks: false } %>
          </div>
        </div>
      </div>
    </div>
    <div class="pageBg">
      <div class="pageBgCircle circleOne"></div>
      <div class="pageBgCircle circleTwo"></div>
      <div class="pageBgCircle circleThree"></div>
    </div>

  </section>
</body>
</html>