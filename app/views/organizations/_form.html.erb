<div class="col-md-4">
  <%= form_for @organization do |f| %>
    <div class="form-group-field">
      <%= f.label :name %><br>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <br/>
    <div class="form-group-field">
      <%= f.label :url %><br>
      <%= f.url_field :url, class: "form-control" %>
    </div>
    <br/>
    <div class="form-group-field">
      <%= f.label :primary_contact_first_name %><br>
      <%= f.text_field :primary_contact_first_name, class: "form-control" %>
    </div>
    <br/>
    <div class="form-group-field">
      <%= f.label :primary_contact_last_name %><br>
      <%= f.text_field :primary_contact_last_name, class: "form-control" %>
    </div>
    <br/>
    <div class="form-group-field">
      <%= f.label :primary_contact_phone %><br>
      <%= f.text_field :primary_contact_phone, class: "form-control" %>
    </div>
    <br/>
    <div class="form-group-field">
      <%= f.label :primary_contact_email %><br>
      <%= f.email_field :primary_contact_email, class: "form-control" %>
    </div>
    </br>
    <div class="form-group">
      <%= f.submit class: "btn btn-primary" %>
    </div>  
  <% end %>
</div>