            <th class="font-weight-bold text-center">Amount</th>
          </tr>
        </thead>
        <tbody>
          <% results['square_feet_requested'].keys.each do |year| %>
            <% results['square_feet_requested'][year].keys.each do |typeId| %>
              <tr>
                <td class="text-center"><%= year %></td>
                <td class="text-center"><%= typeId %></td>
                <td class="text-center"><%= results['square_feet_requested'][year][typeId].count %></td>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

</div>