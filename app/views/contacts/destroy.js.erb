$('#contactsList').html('');
$('#contactsList').html('<%= escape_javascript render partial: 'contacts', locals: {contacts: @contacts} %>');
$('#contactFormCenter').modal('hide');
$('.pagination').find('a').each(function() {
    var a = $(this).attr("href").split("?")[1];
    $(this).attr('href', "contacts?" + a);
    $(this).attr("data-turbolinks", false);
});